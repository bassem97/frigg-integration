        "@friggframework/errors": "^1.1.3",
        "@friggframework/integrations": "^1.0.10",
        "@friggframework/logs": "^1.0.7",
        "@friggframework/module-plugin": "^1.0.13",
        "@hapi/boom": "^7.4.11",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "express-async-handler": "^1.1.4",
